<form method="post" action="{{ path('app_produit_delete', {'numProduit': produit.numProduit}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ produit.numProduit) }}">
    <button class="btn">Delete</button>
</form>
